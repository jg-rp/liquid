            </section>

            <!-- Dynamic Content with Conditions -->
            <section>
                <h3>Items List</h3>
                {% assign items_size = items | size %}
                {% if items_size > 0 %}
                    {% for item in items %}
                        <div class="item">
                            <h4>{{ item.title | capitalize }}</h4>
                            <p>{{ item.description | escape }}</p>
                            <p>Price: {{ item.price | ceil }} USD</p>
                            {% if item.price > 50 %}
                                <p>This is a premium item.</p>
                            {% else %}
                                <p>This is a budget-friendly item.</p>
                            {% endif %}
                        </div>
                    {% endfor %}
                {% else %}
                    <p>No items are available at the moment.</p>
                {% endif %}
            </section>

            <section>
                <h3>Featured Post</h3>
                {% assign posts_size = posts | size %}
                {% if posts_size > 0 %}
                    {% assign featured_post = posts[0] %}
                    <h3>{{ featured_post.title }}</h3>
                    <p>{{ featured_post.content }}</p>
                    <a href="{{ featured_post.url }}">link</a>
                {% else %}
                    <p>No featured content available.</p>
                {% endif %}
            </section>

            <section>
                <h3>Latest Posts</h3>
                {% assign posts_size = posts | size %}
                {% if posts_size > 0 %}
                    {% for post in posts %}
                        <article>
                            <h4>{{ post.title | capitalize }}</h4>
                            <p>{{ post.content | slice: 0, 150 }}</p>
                            <p><a href="{{ post.url }}">Read More</a></p>
                            {% assign post_content_size = post.content | size %}
                            {% if post_content_size > 300 %}
                                <p>This is a lengthy post.</p>
                            {% else %}
                                <p>This is a quick read.</p>
                            {% endif %}
                        </article>
                    {% endfor %}
                {% else %}
                    <p>No posts available at this time.</p>
                {% endif %}
            </section>
